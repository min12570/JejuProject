<script>
    var seq = 12447;

    $(document).ready(function () {
        getEventView();
    });

    // 글 상세보기.
    function getEventView() {
        $.ajax({
            url: "/Json/WebBoard/WebBoard.aspx"
            , type: "POST"
            , data: {
                type: "VIEW"
                , seq: seq
            }
            , dataType: "JSON"
            , cache: false
            , async: false
            , success: function (data) {
                setEventView(data);
            }
            , error: function (e) {
                // e.responseText;
            }
        });
    }

    // 글 상세보기 데이터 삽입.
    function setEventView(data) {
        if (!$.isEmptyObject(data.data)) {
            // 제목
            $("h4.view_hd").html(data.data.subject);

            // 등록일 형식 변경
            var regdate = data.data.regdate.split(" ");
            var sdate = data.data.sdate.split(" ");
            var edate = data.data.edate.split(" ");
            $("span.view_date").html(sdate[0] + " ~ " + edate[0]);
            //$("span.view_date").html( regdate );

            // 조회수
            $("span.view_viewno").html(data.data.viewno);

            // 분류
            var pagetype = data.pagetype.split("_");
            $("span.view_pagetype").html(pagetype[0]);

            // 내용
            $("div.bbs_cont").html(data.data.content);
            $("div.bbs_cont img").removeAttr("title");

            // 이전글
            var prevStr = !$.isEmptyObject(data.prev) ? '<a href="Package_view?seq=' + data.prev.seq + '">' + data.prev.subject + '</a>' : '이전 글이 없습니다.';
            $("p.prev span").html(prevStr);

            // 다음글 
            var nextStr = !$.isEmptyObject(data.next) ? '<a href="Package_view?seq=' + data.next.seq + '">' + data.next.subject + '</a>' : '다음 글이 없습니다.';
            $("p.next span").html(nextStr);
        }
        else {
            alert("데이터가 존재하지 않습니다.");
            history.back();
        }
    }
</script>


